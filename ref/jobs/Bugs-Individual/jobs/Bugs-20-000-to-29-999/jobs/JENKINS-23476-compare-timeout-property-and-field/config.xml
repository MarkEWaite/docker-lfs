  <builders/>
  <publishers>
    <org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder plugin="groovy-postbuild@2.3.1">
      <script plugin="script-security@1.24">
        <script>def TIMEOUT = org.jenkinsci.plugins.gitclient.CliGitAPIImpl.TIMEOUT
def timeOut = Integer.getInteger(&quot;org.jenkinsci.plugins.gitclient.Git.timeOut&quot;, TIMEOUT)

if (TIMEOUT != timeOut) {
    manager.addWarningBadge(&quot;TIMEOUT == &quot; + TIMEOUT + &quot; timeOut == &quot; + timeOut)
    manager.buildUnstable()
}</script>
        <sandbox>true</sandbox>
      </script>
      <behavior>2</behavior>
      <runForMatrixParent>false</runForMatrixParent>
    </org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder>
  </publishers>
  <buildWrappers/>
